{
  "name": "barkpark-backend",
  "version": "1.0.0",
  "description": "Backend API for BarkPark dog social network app",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:prod": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "node scripts/railway-migrate.js",
    "migrate:seed": "node scripts/railway-migrate.js --seed",
    "migrate:force": "node scripts/railway-migrate.js --force",
    "migrate:dogs": "node scripts/run-dogs-migration.js",
    "generate-test-token": "node scripts/generate-test-token.js"
  },
  "keywords": [
    "dogs",
    "social",
    "api",
    "parks"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1498.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "socket.io": "^4.7.4"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ]
  }
}
